<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Swarming\SubscribePro\Model\Config\Source\ThirdPartyPaymentMethod">
        <arguments>
            <argument name="supportedMethods" xsi:type="array">
                <item name="cardconnect_cc" xsi:type="string">cardconnect_cc</item>
                <item name="cardconnect_hpp" xsi:type="string">cardconnect_hpp</item>
                <item name="cardconnect_apple_pay" xsi:type="string">cardconnect_apple_pay</item>
                <item name="cardconnect_google_pay" xsi:type="string">cardconnect_google_pay</item>
