                <item name="cardconnect_hpp" xsi:type="object">Rightpoint\SubscriptionsExternalVault\Service\PaymentProfileDataBuilder</item>
                <item name="cardconnect_apple_pay" xsi:type="object">Rightpoint\SubscriptionsExternalVault\Service\PaymentProfileDataBuilder</item>
